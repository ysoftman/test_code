#!/bin/bash
# git var -l 로도 환경변수를 파악할 수 있다.
echo "-- Git Internals - Environment Variables --"
echo "GIT_EXEC_PATH=$GIT_EXEC_PATH"
echo "HOME=$HOME"
echo "GIT_CONFIG_NOSYSTEM=$GIT_CONFIG_NOSYSTEM"
echo "GIT_PAGER=$GIT_PAGER"
echo "GIT_EDITOR=$GIT_EDITOR"
echo "GIT_DIR=$GIT_DIR"
echo "GIT_GLOB_PATHSPECS=$GIT_GLOB_PATHSPECS"
echo "GIT_LITERAL_PATHSPECS=$GIT_LITERAL_PATHSPECS"
echo "GIT_ICASE_PATHSPECS=$GIT_ICASE_PATHSPECS"
echo "GIT_AUTHOR_NAME=$GIT_AUTHOR_NAME"
echo "GIT_AUTHOR_EMAIL=$GIT_AUTHOR_EMAIL"
echo "GIT_AUTHOR_DATE=$GIT_AUTHOR_DATE"
echo "GIT_COMMITTER_NAME=$GIT_COMMITTER_NAME"
echo "GIT_COMMITTER_EMAIL=$GIT_COMMITTER_EMAIL"
echo "GIT_COMMITTER_DATE=$GIT_COMMITTER_DATE"
echo "EMAIL=$EMAIL"
echo "GIT_SSL_NO_VERIFY=$GIT_SSL_NO_VERIFY"
echo "GIT_HTTP_LOW_SPEED_TIME=$GIT_HTTP_LOW_SPEED_TIME"
echo "GIT_HTTP_USER_AGENT=$GIT_HTTP_USER_AGENT"
echo "GIT_DIFF_OPTS=$GIT_DIFF_OPTS"
echo "GIT_EXTERNAL_DIFF=$GIT_EXTERNAL_DIFF"
echo "GIT_DIFF_PATH_COUNTER=$GIT_DIFF_PATH_COUNTER"
echo "GIT_EXTERNAL_DIFF=$GIT_EXTERNAL_DIFF"
echo "GIT_DIFF_PATH_TOTAL=$GIT_DIFF_PATH_TOTAL"
echo "GIT_DIFF_PATH_COUNTER=$GIT_DIFF_PATH_COUNTER"
echo "GIT_MERGE_VERBOSITY=$GIT_MERGE_VERBOSITY"
echo "GIT_TRACE=$GIT_TRACE"
echo "GIT_TRACE_PACK_ACCESS=$GIT_TRACE_PACK_ACCESS"
echo "GIT_TRACE_PACKET=$GIT_TRACE_PACKET"
echo "GIT_TRACE_PERFORMANCE=$GIT_TRACE_PERFORMANCE"
echo "GIT_TRACE_SETUP=$GIT_TRACE_SETUP"
echo "GIT_SSH=$GIT_SSH"
echo "GIT_ASKPASS=$GIT_ASKPASS"
echo "GIT_NAMESPACE=$GIT_NAMESPACE"
echo "GIT_FLUSH=$GIT_FLUSH"
echo "GIT_REFLOG_ACTION=$GIT_REFLOG_ACTION"
